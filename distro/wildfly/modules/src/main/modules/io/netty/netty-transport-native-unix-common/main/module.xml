<!-- custom module to match netty versions   -->
<module name="io.netty.netty-transport-native-unix-common" xmlns="urn:jboss:module:1.9">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <resource-root path="netty-transport-native-unix-common-@version.netty@-linux-x86_64.jar"/>
        <resource-root path="netty-transport-native-unix-common-@version.netty@-linux-aarch_64.jar"/>
        <resource-root path="netty-transport-native-unix-common-@version.netty@-osx-x86_64.jar"/>
        <resource-root path="netty-transport-native-unix-common-@version.netty@-osx-aarch_64.jar"/>
    </resources>

    <dependencies>
        <module name="io.netty.netty-common"/>
        <module name="io.netty.netty-buffer"/>
        <module name="io.netty.netty-transport"/>
    </dependencies>
</module>
