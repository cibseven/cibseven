<!-- custom module to fix CVE-2025-58057  -->
<module name="io.netty.netty-codec" xmlns="urn:jboss:module:1.9">
    <properties>
        <property name="jboss.api" value="private"/>
    </properties>

    <resources>
        <resource-root path="netty-codec-@version.netty@.jar"/>
    </resources>

    <dependencies>
        <module name="io.netty.netty-common"/>
        <module name="io.netty.netty-buffer"/>
        <module name="io.netty.netty-transport"/>
    </dependencies>
</module>